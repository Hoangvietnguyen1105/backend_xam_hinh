<html lang="en">
<head>
    
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>CRUD</title>
    <!-- Latest compiled and minified CSS -->
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/css/bootstrap.min.css">

<!-- jQuery library -->
<script src="https://cdn.jsdelivr.net/npm/jquery@3.6.1/dist/jquery.slim.min.js"></script>

<!-- Popper JS -->
<script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"></script>

<!-- Latest compiled JavaScript -->
<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/js/bootstrap.bundle.min.js"></script>
</head>
<body>
    <form action="/post-crud-test" method="post">
        <div class="form-row">
          <div class="form-group col-md-6">
            <label for="inputEmail4">currentNumber</label>
            <input type="email" class="form-control" id="email"  placeholder="Email" name="currentNumber">
          </div>
          <div class="form-group col-md-6">
            <label for="inputPassword4">maxNumber</label>
            <input type="password" class="form-control" id="password" placeholder="Password" name="maxNumber">
          </div>
          <div class="form-group col-md-6">
            <label for="inputEmail4">date</label>
            <input type="text" class="form-control" id="firstName"  placeholder="Email" name="date">
          </div>
          <div class="form-group col-md-6">
            <label for="inputEmail4">timeType</label>
            <input type="text" class="form-control" id="lastName"  placeholder="Email" name="timeType">
          </div>
          <div class="form-group col-md-6">
            <label for="inputEmail4">doctorId</label>
            <input type="text" class="form-control" id="address"  placeholder="Email" name="doctorId">
          </div>
        </div>
        
        
        <button type="submit" class="btn btn-primary">Sign in</button>
      </form>
</body>
</html>